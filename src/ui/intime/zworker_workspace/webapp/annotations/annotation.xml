				<Annotation Term="UI.LineItem">
					<Collection>

						<Record Type="UI.DataField">
							<PropertyValue Property="Value"
								Path="PartnerImageURL"/>
							<Annotation Term="UI.Importance"
								EnumMember="UI.ImportanceType/High"/>
						</Record>

						<Record Type="UI.DataField">
							<PropertyValue Property="Value"
								Path="SubTaskID" />
						</Record>

						<Record Type="UI.DataField">
							<PropertyValue Property="Value"
								Path="SubTaskStatus" />
						</Record>


						<Record Type="UI.DataField">
							<PropertyValue Property="Value"
								Path="TaskID" />
						</Record>
						<Record Type="UI.DataField">
							<PropertyValue Property="Value"
								Path="StageID" />
						</Record>

						<Record Type="UI.DataField">
							<PropertyValue Property="Value"
								Path="ProjectID" />
						</Record>

						<Record Type="UI.DataField">
							<PropertyValue Property="Value"
								Path="StartDate" />
						</Record>

						<Record Type="UI.DataField">
							<PropertyValue Property="Value"
								Path="EndDate" />
						</Record>

						<Record Type="UI.DataField">
							<PropertyValue Property="Value"
								Path="EstimateTime" />
						</Record>

						<Record Type="UI.DataField">
							<PropertyValue Property="Value"
								Path="WorkDescr" />
						</Record>



					</Collection>
				</Annotation>
			</Annotations>

			<Annotations Target="ZINT_UI_WORKER_WORKSPACE_SRV.ZSNN_WORKER_LISTType">

				<Annotation Term="UI.FieldGroup"
					Qualifier="Activities">
					<Record>
						<PropertyValue Property="Data">
							<Collection>
								<Record Type="UI.DataField">
									<PropertyValue Property="Value"
										Path="OpenAssignedSubTask"/>
								</Record>
							</Collection>
						</PropertyValue>
						<PropertyValue Property="Label"
							String="Текущиче активности"/>
					</Record>
				</Annotation>

				<Annotation Term="UI.FieldGroup"
					Qualifier="NameInfoHeader">
					<Record>
						<PropertyValue Property="Data">
							<Collection>
								<Record Type="UI.DataField">
									<PropertyValue Property="Value"
										Path="Surname"/>
									<Annotation Term="UI.Importance"
										EnumMember="UI.ImportanceType/Low"/>
								</Record>
								<Record Type="UI.DataField">
									<PropertyValue Property="Value"
										Path="Name"/>
									<Annotation Term="UI.Importance"
										EnumMember="UI.ImportanceType/High"/>
								</Record>

								<Record Type="UI.DataField">
									<PropertyValue Property="Value"
										Path="Branch"/>
									<Annotation Term="UI.Importance"
										EnumMember="UI.ImportanceType/Low"/>
								</Record>
								<Record Type="UI.DataField">
									<PropertyValue Property="Value"
										Path="PositionID"/>
									<Annotation Term="UI.Importance"
										EnumMember="UI.ImportanceType/High"/>
								</Record>

							</Collection>
						</PropertyValue>
						<PropertyValue Property="Label"
							String="Общая инофрмация"/>
					</Record>
				</Annotation>

				<Annotation Term="UI.Facets">
					<Collection>
						<Record Type="UI.ReferenceFacet">
							<PropertyValue Property="Label"
								String="Назначенные задачи"/>
							<PropertyValue Property="Target"
								AnnotationPath="to_AssignedSubTask/@UI.LineItem" />
						</Record>
					</Collection>
				</Annotation>




				<Annotation Term="UI.HeaderFacets">
					<Collection>

						<Record Type="UI.ReferenceFacet">
