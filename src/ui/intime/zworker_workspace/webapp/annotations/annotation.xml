	<edmx:DataServices>
		<Schema xmlns="http://docs.oasis-open.org/odata/ns/edm">


			<Annotations Target="ZINT_UI_WORKER_WORKSPACE_SRV.ZSNN_WORKER_ASSIGNED_SUBTASKType/PartnerImageURL">
