{
  "_version": "1.12.0",
  "sap.app": {
    "id": "intheme.zworker_schedule",
    "type": "application",
    "dataSources": {
      "mainService": {
        "uri": "/sap/opu/odata/sap/ZWORKER_VISIT_SCHEDULE_SRV",
        "type": "OData",
        "settings": {
          "odataVersion": "2.0",
          "annotations": ["annotation"]
        }
      },
      "annotation": {
        "type": "ODataAnnotation",
        "uri": "localService/annotation.xml",
        "settings": {
          "localUri": "localService/annotation.xml"
        }
      }
    },
    "i18n": "i18n/i18n.properties",
    "applicationVersion": {
      "version": "1.0.0"
    },
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "sourceTemplate": {
      "id": "ui5template.basicSAPUI5ApplicationProject",
      "version": "1.40.12"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "icons": {
      "icon": "",
      "favIcon": "",
      "phone": "",
      "phone@2": "",
      "tablet": "",
      "tablet@2": ""
    },
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    },
    "supportedThemes": ["sap_hcb", "sap_belize"],
    "fullWidth": true
  },
  "sap.ui5": {
    "flexEnabled": false,
    "rootView": {
      "viewName": "intheme.zworker_schedule.view.App",
      "type": "XML",
      "async": true,
      "id": "App"
    },
    "dependencies": {
      "minUI5Version": "1.71.22",
      "libs": {
        "sap.ui.unified": {},
        "sap.ui.layout": {},
        "sap.ui.core": {},
        "sap.m": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "": {
        "dataSource": "mainService",
        "settings": {
          "serviceUrlParams": {
            "sap-client": "300"
          },
          "defaultBindingMode": "TwoWay",
          "useBatch": true,
          "defaultCountMode": "Inline"
        }
      },
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "intheme.zworker_schedule.i18n.i18n"
        }
      },
      "state": {
        "type": "sap.ui.model.json.JSONModel",
        "settings": {
          "isDraft": false,
          "icons": {
            "goHome": "sap-icon://home"
          },
          "layout": "OneColumn",
          "proj_filter": "",
          "ConfigSTMode" : "None",
          "rebindProj": false,
          "currentTab": "common",
          "showNewInterval": false,
          "smartTabConf": {
            "PositionTab": "PositionConfST",
            "WorkerTab": "WorkerConfST"
          }
        }
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "async": true,
        "viewPath": "intheme.zworker_schedule.view",
        "controlAggregation": "beginColumnPages",
        "controlId": "layout"
      },
      "routes": [
        {
          "name": "WorklistRoute",
          "pattern": "",
          "target": ["WorklistTarget"]
        },

        {
          "name": "DetailRoute",
          "pattern": "detail/:?query:",
          "target": ["DetailTarget", "WorklistTarget"]
        }
      ],
      "targets": {
        "WorklistTarget": {
          "viewType": "XML",
          "transition": "fade",
          "viewName": "Worklist",
          "ViewID": "Worklist",
          "controlAggregation": "beginColumnPages",
          "viewLevel": 1
        },

        "DetailTarget": {
          "viewName": "Detail",
          "viewId": "Detail",
          "viewLevel": 2,
          "controlAggregation": "midColumnPages"
        }
      }
    }
  }
}
